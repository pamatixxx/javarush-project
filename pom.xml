<project xmlns="http://maven.apache.org/POM/4.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://maven.apache.org/POM/4.0.0 https://maven.apache.org/xsd/maven-4.0.0.xsd">
    <modelVersion>4.0.0</modelVersion>

    <!-- Уникальные идентификаторы вашего проекта -->
    <groupId>com.javarush.project</groupId>
    <artifactId>javarush-project</artifactId>
    <version>1.0-SNAPSHOT</version>
    <packaging>jar</packaging> <!-- Тип упаковки -->

    <!-- Глобальные свойства для проекта -->
    <properties>
        <maven.compiler.source>11</maven.compiler.source>
        <maven.compiler.target>11</maven.compiler.target>
        <!-- Дополнительное свойство для версии JavaFX -->
        <javafx.version>17</javafx.version>
    </properties>

    <!-- Секция ваших зависимостей (включая пример для JavaFX) -->
    <dependencies>
        <!-- Пример зависимости для JavaFX Controls (если вы используете JavaFX) -->
        <dependency>
            <groupId>org.openjfx</groupId>
            <artifactId>javafx-controls</artifactId>
            <version>${javafx.version}</version>
        </dependency>
    </dependencies>
</project>
